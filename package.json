{
  "name": "@lokalise/zod-extras",
  "version": "2.0.1",
  "author": {
    "name": "Lokalise",
    "url": "https://lokalise.com/"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/lokalise/zod-extras.git"
  },
  "homepage": "https://github.com/lokalise/zod-extras",
  "files": [
    "dist/**",
    "LICENSE",
    "README.md"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "rimraf dist && vite build",
    "build:ts": "shx rm -rf ./dist && tsc",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:ci": "npm run lint && npm run test:coverage",
    "lint": "eslint --cache --max-warnings=0 . && prettier --check --log-level warn src \"**/*.{json,md,ts,tsx}\" && tsc --noEmit",
    "lint:fix": "prettier --write src \"**/*.{json,md,ts,tsx}\" --log-level=warn && eslint . --fix",
    "version": "auto-changelog -p && git add CHANGELOG.md"
  },
  "dependencies": {},
  "devDependencies": {
    "@lokalise/package-vite-config": "^2.0.0",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "auto-changelog": "^2.4.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^28.3.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "shx": "^0.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.10",
    "zod": "^3.22.4"
  }
}
